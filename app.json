{
  "expo": {
    "name": "مكتب أجوان",
    "slug": "malltoall_navigation_app",
    "version": "1.0.0",
    "splash": {
      "image": "./assets/images/splash.jpeg",
      "resizeMode": "contain", 
      "backgroundColor": "#ffffff"
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ajwan.expoapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "We need your location to display the map."
      }
    },
    
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "POST_NOTIFICATIONS"
      ],
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abdullah_lahham123.malltoall_navigation_app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      
      [
        "expo-notifications",
        {
          "mode": "automatic",
          "ios": {
            "allowAlert": true,
            "allowBadge": true,
            "allowSound": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2aa8d1e9-4ad7-4d0f-b8fe-6b8e8e6fc32c"
      }
    },
    "owner": "abdullah_lahham123",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2aa8d1e9-4ad7-4d0f-b8fe-6b8e8e6fc32c"
    }
  }
}


